<%= form_for(@user, url: yield(:url)) do |f| %>

  <%= render 'shared/errors', object: f.object %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= #f.label %>
  <%= #f.text_field %>

  <%= f.submit yield(:buton_text) %>

<% end %>